<manifest package="com.github.jinatonic.confetti.sample"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".FallingConfettiFromTopActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".FallingConfettiFromPointActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ExplosionActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ShimmeringActivity"
            android:parentActivityName=".MainActivity" />

    </application>

</manifest>
